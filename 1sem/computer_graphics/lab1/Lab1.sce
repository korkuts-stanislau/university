x=[2,3,2,8,7,8,2]
y=[2,8,14,14,8,2,2]
x1=[3.5,6.5,3.5,6.5,3.5]
y1=[12,12,4,4,12]
plot(0,0)
plot(20,20)
plot(x,y)
plot(x1,y1)

//Перемещение

x=x+10
x1=x1+10
y=y+10
y1=y1+10
plot(x,y)
plot(x1,y1)

//Растягивание

x=x*3-5
x1=x1*3-5
y=y*3-20
y1=y1*3-20
plot(x,y)
plot(x1,y1)

//Поворот
x=[2,3,2,8,7,8,2]
y=[2,8,14,14,8,2,2]
x1=[3.5,6.5,3.5,6.5,3.5]
y1=[12,12,4,4,12]
a=45
x=x*cos(a)-y*sin(a)
y=x*cos(a)+y*sin(a)
x1=x1*cos(a)-y1*sin(a)
y1=x1*cos(a)+y1*sin(a)
y=y+20
y1=y1+20

plot(x,y)
plot(x1,y1)
